{
    "name": "signpostmarv/daft-nested-object",
    "description": "nested sets implementation for daft object",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "SignpostMarv",
            "email": "me@signpostmarv.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "SignpostMarv\\DaftObject\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SignpostMarv\\DaftMagicPropertyAnalysis\\": "./vendor/signpostmarv/daft-magic-property-analysis/src/",
            "SignpostMarv\\DaftMagicPropertyAnalysis\\PHPStan\\": "./vendor/signpostmarv/daft-magic-property-analysis/PHPStan/",
            "SignpostMarv\\DaftObject\\DaftNestedObject\\PHPStan\\": "./PHPStan/",
            "SignpostMarv\\DaftObject\\DaftNestedObject\\Tests\\": "./Tests/",
            "SignpostMarv\\DaftObject\\": "./vendor/signpostmarv/daft-object/tests-src/",
            "SignpostMarv\\DaftObject\\PHPStan\\": "./vendor/signpostmarv/daft-object/PHPStan/",
            "SignpostMarv\\DaftObject\\Tests\\": "./vendor/signpostmarv/daft-object/Tests/"
        }
    },
    "config": {
        "classmap-authoritative": true,
        "sort-packages": true
    },
    "scripts": {
        "infection": [
            "@tests",
            "infection"
        ],
        "tests": [
            "phpunit -vvvv --coverage-clover=phpunit.clover --stop-on-error --stop-on-warning --stop-on-failure",
            "phpstan analyse -c ./phpstan.neon .php_cs.dist src Tests PHPStan",
            "psalm",
            "@other-tests"
        ],
        "other-tests": [
            "phpcpd src Tests PHPStan",
            "phpmnd src",
            "phpmnd Tests",
            "phpmnd PHPStan",
            "composer-require-checker check ./composer.json",
            "php-cs-fixer fix --verbose --allow-risky=yes --dry-run"
        ]
    },
    "require": {
        "php": "^7.1",
        "signpostmarv/daft-object": "^1.11"
    },
    "require-dev": {
        "infection/infection": "^0.12.0",
        "maglnet/composer-require-checker": "^1.1",
        "phpstan/phpstan": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-strict-rules": "^0.11.0",
        "phpunit/phpunit": "^7.5",
        "povils/phpmnd": "^2.1",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.1",
        "signpostmarv/php-cs-fixer-config": "^0.6.1",
        "vimeo/psalm": "dev-master"
    }
}
